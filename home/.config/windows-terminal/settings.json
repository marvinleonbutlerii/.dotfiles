    "copyFormatting": "none",
    "copyOnSelect": true,
    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
    "keybindings": [
        { "id": "User.copy", "keys": "ctrl+c" },
        { "id": "User.paste", "keys": "ctrl+v" },
        { "id": "User.find", "keys": "ctrl+shift+f" },
        { "id": "User.newTab", "keys": "ctrl+t" },
        { "id": "User.closePane", "keys": "ctrl+w" },
        { "id": "User.splitPane.horizontal", "keys": "alt+shift+-" },
        { "id": "User.splitPane.vertical", "keys": "alt+shift+\\" },
        { "id": "User.moveFocus.left", "keys": "alt+h" },
        { "id": "User.moveFocus.right", "keys": "alt+l" },
        { "id": "User.moveFocus.up", "keys": "alt+k" },
        { "id": "User.moveFocus.down", "keys": "alt+j" },
        { "id": "User.resizePane.left", "keys": "alt+shift+h" },
        { "id": "User.resizePane.right", "keys": "alt+shift+l" },
        { "id": "User.resizePane.up", "keys": "alt+shift+k" },
        { "id": "User.resizePane.down", "keys": "alt+shift+j" },
        { "id": "User.switchToTab.0", "keys": "alt+1" },
        { "id": "User.switchToTab.1", "keys": "alt+2" },
        { "id": "User.switchToTab.2", "keys": "alt+3" },
        { "id": "User.switchToTab.3", "keys": "alt+4" },
        { "id": "User.switchToTab.4", "keys": "alt+5" }
    ],
    "newTabMenu": [
        { "type": "remainingProfiles" }
    ],
